generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("POSTGRES_PRISMA_URL")
  directUrl         = env("POSTGRES_URL_NON_POOLING")
  shadowDatabaseUrl = env("POSTGRES_URL_NON_POOLING")
}

model Thing {
  id       Int       @id @default(autoincrement())
  title    String?
  content  String?
  when     DateTime?
  at       String?
  priority Int?
  done     Boolean?
}
